{
  "name": "next-portfolio",
  "version": "1.0.0",
  "description": "My portfolio built with Next.js",
  "dependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/js-cookie": "^2.2.6",
    "@types/lusca": "^1.6.1",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.7.21",
    "@types/node": "^10.17.24",
    "@types/nprogress": "0.0.29",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/styled-jsx": "^2.2.8",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "dotenv": "^6.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-session": "^1.17.1",
    "isomorphic-unfetch": "^2.1.1",
    "js-cookie": "^2.2.1",
    "lusca": "^1.6.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.9.17",
    "next": "^11.1.3",
    "nprogress": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^1.19.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-next && npm run build-server",
    "serve": "node .server/index.js",
    "watch": "concurrently -k \"tsc -w -p tsconfig.server.json\" \"nodemon .server/index.js\"",
    "build-next": "next build",
    "build-server": "tsc -p tsconfig.server.json",
    "tslint": "npm run tslint-next && npm run tslint-server",
    "tslint-next": "tslint -c tslint.json -p tsconfig.json",
    "tslint-server": "tslint -c tslint.json -p tsconfig.server.json",
    "tslint-fix": "tslint-next-fix && tslint-server-fix",
    "tslint-next-fix": "tslint --fix -c tslint.json -p tsconfig.json",
    "tslint-server-fix": "tslint --fix -c tslint.json -p tsconfig.server.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect .server/index.js",
    "watch-debug": "concurrently -k \"tsc -w -p tsconfig.server.json\" \"nodemon --inspect .server/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/S-a-l-a-d/next-portfolio.git"
  },
  "author": "Salad <xa.lach@yahoo.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/S-a-l-a-d/next-portfolio/issues"
  },
  "homepage": "https://github.com/S-a-l-a-d/next-portfolio#readme"
}
